SELECT SALESFORCE_ID
, CASE WHEN C.MAILINGPOSTALCODE in (  '10002', '10003', '10009', '10013', '10025', '10026', '10027', '10029', '10030', '10031', '10032'
                                , '10033', '10034', '10035', '10037', '10039', '10040', '10301', '10303', '10304', '10310', '10451'
