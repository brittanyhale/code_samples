                                , '11208', '11212', '11216', '11220', '11221', '11224', '11226', '11232', '11233', '11235', '11236'
                                , '11237', '11238', '11239', '11368', '11369', '11412', '11419', '11420', '11421', '11423', '11429'
                                , '11432', '11433', '11434', '11435', '11436', '11691', '11692', '11693', '11694'
                                , '10001', '10004', '10005', '10006', '10007', '10010', '10011', '10012', '10014', '10016', '10017'
                                , '10018', '10019', '10021', '10022', '10023', '10024', '10028', '10036', '10038', '10044', '10065'
                                , '10069', '10075', '10128', '10280', '10282', '10302', '10305', '10306', '10307', '10308', '10309'
                                , '10312', '10314', '10461', '10462', '10464', '10465', '10470', '10471', '10475', '11004', '11102'
                                , '11103', '11104', '11105', '11106', '11109', '11201', '11204', '11209', '11210', '11211', '11213'
                                , '11214', '11215', '11217', '11218', '11219', '11222', '11223', '11225', '11228', '11229', '11230'
                                , '11231', '11234', '11354', '11355', '11356', '11357', '11358', '11360', '11361', '11362', '11363'
                                , '11364', '11365', '11366', '11367', '11370', '11372', '11373', '11374', '11375', '11377', '11378'
                                , '11379', '11385', '11411', '11413', '11414', '11415', '11416', '11417', '11418', '11422', '11426'
                                , '11427', '11428', '11697'
                               )
     THEN 'IN NYC'
     ELSE 'NOT IN NYC' END AS MAIN_ADDRESS
, CASE WHEN C.OtherPostalCode in (  '10002', '10003', '10009', '10013', '10025', '10026', '10027', '10029', '10030', '10031', '10032'
                                , '10033', '10034', '10035', '10037', '10039', '10040', '10301', '10303', '10304', '10310', '10451'
                                , '10452', '10453', '10454', '10455', '10456', '10457', '10458', '10459', '10460', '10463', '10466'
                                , '10467', '10468', '10469', '10472', '10473', '10474', '11101', '11203', '11205', '11206', '11207'
                                , '11208', '11212', '11216', '11220', '11221', '11224', '11226', '11232', '11233', '11235', '11236'
                                , '11237', '11238', '11239', '11368', '11369', '11412', '11419', '11420', '11421', '11423', '11429'
                                , '11432', '11433', '11434', '11435', '11436', '11691', '11692', '11693', '11694'
                                , '10001', '10004', '10005', '10006', '10007', '10010', '10011', '10012', '10014', '10016', '10017'
                                , '10018', '10019', '10021', '10022', '10023', '10024', '10028', '10036', '10038', '10044', '10065'
                                , '10069', '10075', '10128', '10280', '10282', '10302', '10305', '10306', '10307', '10308', '10309'
                                , '10312', '10314', '10461', '10462', '10464', '10465', '10470', '10471', '10475', '11004', '11102'
                                , '11103', '11104', '11105', '11106', '11109', '11201', '11204', '11209', '11210', '11211', '11213'
                                , '11214', '11215', '11217', '11218', '11219', '11222', '11223', '11225', '11228', '11229', '11230'
                                , '11231', '11234', '11354', '11355', '11356', '11357', '11358', '11360', '11361', '11362', '11363'
                                , '11364', '11365', '11366', '11367', '11370', '11372', '11373', '11374', '11375', '11377', '11378'
                                , '11379', '11385', '11411', '11413', '11414', '11415', '11416', '11417', '11418', '11422', '11426'
                                , '11427', '11428', '11697'
                               )
     THEN 'IN NYC'
     ELSE 'NOT IN NYC' END AS OTHER_ADDRESS
INTO ##STATE
FROM SF2.CONTACT C
INNER JOIN (SELECT DISTINCT SALESFORCE_CONTACT_ID FROM SF2.RESULT WHERE RECEIVE_PPE_KIT__C = 'Yes') R
ON C.SALESFORCE_ID = R.SALESFORCE_CONTACT_ID

SELECT MAIN_ADDRESS
, OTHER_ADDRESS 
, COUNT(DISTINCT SALESFORCE_ID) COUNT
FROM ##STATE
GROUP BY MAIN_ADDRESS, OTHER_ADDRESS
ORDER BY 3 DESC
